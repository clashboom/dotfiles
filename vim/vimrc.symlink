let g:dotvim_settings = { }
let g:dotvim_settings.version = 1
let g:dotvim_settings.max_column = 80
let g:dotvim_settings.colorscheme = "solarized"
let g:python_host_prog='/usr/bin/python'
 " web, javascript, python, go, scala, ruby
let g:dotvim_settings.plugin_groups_exclude  = ['go', 'scala', 'ruby']

source ~/.dotfiles/vim/vimrc

set guifont:Anonymous\ Pro\ for\ Powerline\ 12

let g:solarized_termcolors=256
set t_Co=256
set background=dark
